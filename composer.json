{
	"name": "nvm/timologio",
	"description": "WooCommerce Timologio",
	"type": "project",
	"license": "GPL-2.0-or-later",
	"require": {
		"php": ">=7.4"
	},
	"autoload": {
		"psr-4": {
			"Nvm\\Timologio\\": "classes"
		},
		"exclude-from-classmap": [
			"vendor/dealerdirect"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"Nvm\\Timologio\\Tests\\": "tests/"
		}
	},
	"authors": [
		{
			"name": "nevma.gr",
			"email": "info@nevma.gr"
		}
	],
	"require-dev": {
		"dealerdirect/phpcodesniffer-composer-installer": "^0.7.2",
		"squizlabs/php_codesniffer": "^3.11",
		"woocommerce/woocommerce-sniffs": "^0.1.3",
		"phpunit/phpunit": "^9.5",
		"mockery/mockery": "^1.5",
		"brain/monkey": "^2.6"
	},
	"scripts": {
		"lint": "phpcs --standard=phpcs.xml.dist",
		"fix": "phpcbf --standard=phpcs.xml.dist",
		"compat": "phpcs --standard=phpcompat.xml.dist",
		"test": "phpunit",
		"test-coverage": "phpunit --coverage-html coverage"
	},
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		},
		"vendor-dir": "prefixed/vendor"
	}
}
